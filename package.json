{
  "name": "wordle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:server": "nest start --debug --watch",
    "build": "nest build && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*.(js|ts|vue)\"",
    "test": "npm run test:client && npm run test:server && npm run test:e2e",
    "test:client": "jest",
    "test:server": "jest --config ./jest-server.config.js",
    "test:e2e": "jest --config ./server/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^7.6.18",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/schematics": "^8.0.7",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@types/node": "^17.0.21",
    "@types/superagent": "^4.1.15",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.21.1",
    "mongoose": "^6.2.3",
    "nest": "^0.1.6",
    "nuxt": "^2.15.8",
    "nuxt-property-decorator": "^2.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "vuex-module-decorators": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/testing": "^7.6.18",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/jest": "^26.0.24",
    "@types/supertest": "^2.0.11",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^26.6.3",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^26.5.6",
    "vue-jest": "^3.0.4"
  }
}
